#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/backend/node_modules/mime/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/backend/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/node_modules:/Users/ishaanbedi/Desktop/aviral/node_modules:/Users/ishaanbedi/Desktop/node_modules:/Users/ishaanbedi/node_modules:/Users/node_modules:/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/backend/node_modules/mime/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/backend/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/node_modules:/Users/ishaanbedi/Desktop/aviral/node_modules:/Users/ishaanbedi/Desktop/node_modules:/Users/ishaanbedi/node_modules:/Users/node_modules:/node_modules:/Users/ishaanbedi/Desktop/aviral/Blog-Central/new/clone/Blog-Central/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
